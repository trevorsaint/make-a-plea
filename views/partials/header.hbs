<header class="with-proposition" id="global-header" role="banner">

  <div class="header-wrapper">

    <div class="header-global">
      <div class="header-logo">
        <a class="content" id="logo" href="https://www.gov.uk" title="Go to the GOV.UK homepage">
          <img src="/public/govuk/images/gov.uk_logotype_crown_invert_trans.png" height="31" width="35" alt="Government crown"> GOV.UK
        </a>
      </div>
    </div>

    <div class="header-proposition">
      <div class="content">
        <div id="proposition-menu">
          <a href="/" id="proposition-name">{{apptitle}}</a>
        </div>
      </div>
    </div>

  </div>

</header>
